# Suppress SES warnings in development
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=false

# Suppress console warnings for SES deprecation messages
REACT_APP_SUPPRESS_SES_WARNINGS=true

# Additional Jest configuration to reduce noise
CI=false

# Disable some verbose webpack output
WEBPACK_HIDE_COMPILATION_WARNINGS=true

# Suppress Node.js deprecation warnings
NODE_NO_WARNINGS=1
NODE_OPTIONS=--no-deprecation

# Suppress SES-specific warnings
SUPPRESS_SES_WARNINGS=true